local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Zirr Hub", "Synapse")

-- 1Credits
local Tab = Window:NewTab("Credits")
local CreditsSection = Tab:NewSection("Credits")


CreditsSection:NewButton("Owner (F9)", "Who Is The Owner Of Zirr Hub? Click F9 To View Print", function()
    print("The Owner Of Zirr Hub is Zir!")
end)


CreditsSection:NewButton("Scripter (F9)", "Who Is The Scripter Of Zirr Hub? Click F9 To View Print", function()
    print("The Scripter Of Zirr Hub is Zir, And Soler!")
end)


CreditsSection:NewButton("Tester (F9)", "Who Is The Tester of Zirr Hub? Click F9 To View Print", function()
    print("The Tester Of Zirr Hub Is Zir, And Soler!")
end)

-- 2LocalPlayer
local Tab = Window:NewTab("Local Player")
local LocalPlayerSection = Tab:NewSection("Local Player")


LocalPlayerSection:NewButton("Reset JP/WS", "Resets Your JumpPower/WalkSpeed To Normal", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
end)


LocalPlayerSection:NewSlider("WalkSpeed", "Changes Your Local WalkSpeed", 500, 1, function(s) -- 500 (MaxValue) | 1 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)


LocalPlayerSection:NewSlider("JumpPower", "Changes Your Local JumpPower", 350, 1, function(s) -- 350 (MaxValue) | 1 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

-- 3CoolStuff
local Main = Window:NewTab("Cool Stuff")
local CoolStuffSection = Main:NewSection("Cool Stuff")


CoolStuffSection:NewToggle("SuperHuman", "Gives you SuperSpeed and SuperJump abilitys!", function(state)
    if state then
       loadstring(game:HttpGet("https://pastebin.com/raw/QqAkMusn"))()
    else
       loadstring(game:HttpGet("https://pastebin.com/raw/gFwxjHPG"))()
    end
end)

-- 4Scripts
local Tab = Window:NewTab("Scripts")
local ScriptsSection = Tab:NewSection("Scripts")


ScriptsSection:NewButton("Infinite Yield", "Executes Infinite Yield", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)


ScriptsSection:NewButton("Fake Items", "Executes A Fake Items Script | V To Close | NOT Made By Me!", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/RobloxHackerProLuaStuff/avatar-editor-thing/main/headless.lua"))()
end)


ScriptsSection:NewButton("Chat Spoofer", "Executes A Chat Spoofer | Not Made By Me!", function()
    loadstring(game:HttpGet(('https://pastebin.com/raw/djBfk8Li'),true))()
end)
